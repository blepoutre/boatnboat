<%= simple_form_for [@user, @boat], html: {class:"mt-5"} do |f| %>
  <%= f.input :name %>
  <%= f.input :category, collection: Boat::CATEGORY %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :picture_url %>
  <%= f.input :address %>
  <%= f.submit 'Valider', class: 'btn btn-flat' %>
<% end %>
